    def decorate(decorators, output, func):
        pos = 0
        wrap = def():
            nonlocal pos
            if pos < decorators.length:
                decorators[pos].expression.print(output)
                pos += 1
                output.with_parens(def():
                    wrap()
                )
            else:
                func()
        wrap()

